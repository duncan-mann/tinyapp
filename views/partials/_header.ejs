<header>
    <nav class="navbar navbar-expand-md navbar-dark bg-dark">


     <!-- TINY APP LOGO/BUTTON  -->
      <a class="navbar-brand" href="/urls">TinyApp</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>

      <!-- My URLS / Create New URL Buttons -->
      <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
        <div class="navbar-nav">
          <a class="nav-item nav-link my-auto" href="/urls">My URLs</a>
          <a class="nav-item nav-link my-auto" href="/urls/new">Create New URL</a>
          <% if (user === undefined) { %>
          <a class="nav-item nav-link my-auto" href="/register">Register</a>
          <a class="nav-item nav-link my-auto" href="/login">Login</a>
        
        <% } else { %>
      
          <p class = "my-auto mx-3">Logged in: <%= user.email %></p>
          <form method = "POST" action = "/logout">
            <input type = "submit" class="btn btn-outline-primary" value="Logout">
          </form>
        </div>
          <% } %>
    </nav>
  </header>